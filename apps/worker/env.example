# Configuration du serveur
PORT=3001
HOST=localhost
NODE_ENV=development

# Base de donn√©es
DATABASE_URL="postgresql://johndoe:randompassword@localhost:5432/elavira_agents?schema=public"

# Redis (pour les queues de jobs)
REDIS_URL=redis://localhost:6379

# Ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=qwen2.5:7b

# RAG
RAG_DB_PATH=./data/vectors
RAG_TABLE_NAME=documents
RAG_CHUNK_SIZE=1000
